{
  "name": "request-rocket",
  "version": "1.1.2",
  "author": "Emarsys <crew-integration@emarsys.com>",
  "description": "Emarsys REST API client",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:mwl": "node .electron-vue/build.js && electron-builder -mwl",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --color --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --color --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "unit:watch": "karma start test/unit/karma.conf.js --single-run false",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Request Rocket",
    "appId": "com.emarsys.request-rocket",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/rocket.icns"
    },
    "win": {
      "icon": "build/icons/rocket.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "codemirror": "5.58.2",
    "crypto-js": "4.0.0",
    "escher-auth": "3.0.0",
    "pretty-bytes": "5.4.1",
    "pretty-ms": "7.0.1",
    "ramda": "0.27.1",
    "request": "2.88.2",
    "uuid": "3.3.2",
    "vue": "2.6.12",
    "vue-codemirror": "4.0.6",
    "vue-electron": "1.0.6",
    "vue-router": "3.4.8",
    "vue-splitpane": "1.0.6",
    "vuex": "3.5.1",
    "vuex-persistedstate": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "@vue/test-utils": "1.1.0",
    "ajv": "6.12.6",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "babili-webpack-plugin": "0.1.2",
    "cfonts": "2.8.6",
    "chai": "4.2.0",
    "chalk": "4.1.0",
    "copy-webpack-plugin": "4.5.3",
    "cross-env": "5.2.0",
    "css-loader": "3.6.0",
    "del": "3.0.0",
    "devtron": "1.4.0",
    "dotenv": "8.2.0",
    "electron": "9.3.3",
    "electron-builder": "22.9.1",
    "electron-debug": "3.1.0",
    "electron-devtools-installer": "3.1.1",
    "eslint": "5.7.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-loader": "2.1.1",
    "eslint-plugin-html": "4.0.6",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-vue": "4.7.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "3.2.0",
    "inject-loader": "4.0.1",
    "karma": "5.2.3",
    "karma-chai": "0.1.0",
    "karma-coverage": "2.0.3",
    "karma-electron": "6.3.1",
    "karma-mocha": "2.0.1",
    "karma-sinon-chai": "2.0.2",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "4.0.2",
    "mini-css-extract-plugin": "1.2.1",
    "mocha": "8.2.0",
    "multispinner": "0.2.1",
    "nock": "13.0.4",
    "node-loader": "1.0.2",
    "prettier": "1.14.3",
    "require-dir": "1.2.0",
    "sinon": "9.2.1",
    "sinon-chai": "3.5.0",
    "spectron": "11.1.0",
    "style-loader": "2.0.0",
    "url-loader": "4.1.1",
    "vue-devtools": "5.1.4",
    "vue-html-loader": "1.2.4",
    "vue-loader": "15.9.4",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.12",
    "webpack": "4.44.2",
    "webpack-cli": "4.1.0",
    "webpack-dev-server": "3.11.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.2.0"
  }
}
